{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outBlob",
      "path": "outcontainer/{rand-guid}",
      "connection": "STORAGE_CONNECTION_STRING",
      "dataType": "binary"
    }
  ]
}
